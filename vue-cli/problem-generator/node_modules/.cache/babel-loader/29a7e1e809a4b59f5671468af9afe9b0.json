? b+1: b;\n\ta = (a == c) ? a+1: a;\n\treturn new LinearEquation(a, b, c, d);\n}\n\nexport function rand_expression(max_val)\n{\n\tlet a = range_rand(max_val);\n\tlet b = range_rand(max_val);\n\tlet c = range_rand(max_val);\n\tlet d = range_rand(max_val);\n\tb = (b == d) ? b+1: b;\n\ta = (a == c) ? a+1: a;\n\treturn new Expression(a, b, c, d);\n}\n\nexport function rand_sign()\n{\n\tlet rand = Math.random();\n\treturn rand >= 0.5 ? '+' : '-';\n}\n\nexport function pad(num, size)\n{\n\tvar s = num+\"\";\n\twhile (s.length < size) s = \"0\" + s;\n\treturn s;\n}\n\nexport function fmt_time(t, show_ms)\n{\n\tlet tmp = t;\n\tlet ms = tmp % 1000;\n\ttmp -= ms;\n\ttmp /= 1000;\n\tlet ss = tmp % 60;\n\ttmp -= ss;\n\tlet mm = tmp / 60;\n\tlet res = pad(mm, 2) + \":\" + pad(ss, 2);\n\n\tif (show_ms)\n\t\tres += \".\" + pad(ms, 3);\n\n\treturn res;\n}\n\n"]}]}